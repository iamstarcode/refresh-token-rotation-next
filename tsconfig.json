{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_document.tsx",
    "context/SessionProvider.tsx",
    "hooks/useSession.js",
    "libs/axios.js",
    "pages/api/auth/[...nextauth].ts",
    ".next/types/**/*.ts",
    "app/head.tsx",
    "app/head.tsx",
    "app/layout.tsx",
    "app/layout.tsx"
  ],
  "exclude": ["node_modules"],
  "types": ["types"]
}
